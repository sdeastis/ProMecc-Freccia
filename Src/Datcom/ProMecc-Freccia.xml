		<iyy unit="SLUG*FT2">    290.9247  </iyy>
		<izz unit="SLUG*FT2">    554.0441  </izz>
		<ixz unit="SLUG*FT2">     17.3561  </ixz>
		<emptywt unit="LBS">    650.3640  </emptywt>
		<location name="CG" unit="FT">
		      <x>         7.87  </x>
		      <y>          .00  </y>
		      <z>         -.83  </z>
		</location>
	</mass_balance>

<!-- **********************************************************************
        UNDERCARRIAGE
     *******************************************************************-->
	<ground_reactions>
		<!-- DATCOM does not calculate ground reactions parameters. -->
	</ground_reactions>

<!-- **********************************************************************
        PROPULSION
     *******************************************************************-->
	<propulsion>

		<engine file="UNKNOWN">
			<location unit="FT">
				<x>       .00 </x>
				<y>       .00 </y>
				<z>       .00 </z>
			</location>
			<orient unit="DEG">
				<roll>       .0000 </roll>
				<pitch>      .0000 </pitch>
				<yaw>        .0000 </yaw>
			</orient>
			<feed> 0 </feed>
			<feed> 1 </feed>
			<thruster file="_prop">
				<location unit="FT">
					<x>       .97 </x>
					<y>       .00 </y>
					<z>     .0000 </z>
				</location>
				<orient unit="DEG">
					<roll>      .0000 </roll>
					<pitch>     .0000 </pitch>
					<yaw>         .00 </yaw>
				</orient>
			</thruster>
		</engine>

		<tank type="FUEL" number="0" >
			<location unit="FT">
				<x>   .00 </x>
				<y>   .00 </y>
				<z>  -.83 </z>
			</location>
			<radius   unit="FT" >      .00 </radius>
			<capacity unit="LBS">      .01 </capacity>
			<contents unit="LBS">      .00 </contents>
			<density  unit="LBS/GAL"> 6.4  </density>
			<priority> 1 </priority>
		</tank>

		<tank type="FUEL" number="1" >
			<location unit="FT">
				<x>   .00 </x>
				<y>   .00 </y>
				<z>  -.83 </z>
			</location>
			<radius   unit="FT" >      .00 </radius>
			<capacity unit="LBS">      .01 </capacity>
			<contents unit="LBS">      .00 </contents>
			<density  unit="LBS/GAL"> 6.4  </density>
			<priority> 1 </priority>
		</tank>

		<!--    THRUST COEFFICIENT
			Datcom uses a different formula than JSBSim does to calculate thrust coefficent. 
			Datcom uses:
				Tc = Total Thrust / ( q * s)
			contained in property: propulsion/total-thrust-coefficient

			whereas JSBSim calculates per-engine thrust coefficient
				Tc = Thrust / ( RPS^2 * D^4 * rho )
			contained in property: propulsion/engine[x]/thrust-coefficient
		-->
		<function name="propulsion/total-thrust-coefficient">
			<description> Total thrust coefficient calculation </description>
			<quotient>
				<property>propulsion/engine/thrust-lbs</property>
				<property>aero/qbar-area</property>
			</quotient>
		</function>

	</propulsion>
	

<!-- **********************************************************************
        AUTOPILOT
     *******************************************************************-->
   <!-- This is provided so that you can write an autopilot 
        model for your aircraft, based on a generic autopilot      -->

    <autopilot file="autopilot"/>

<!-- **********************************************************************
        FLIGHT CONTROLS
     *******************************************************************-->
	<flight_control name="FCS: ProMecc-Freccia">

		<channel name="Pitch">
			<summer name="fcs/pitch-control-sum">
				<input> fcs/elevator-cmd-norm </input>
				<input> fcs/pitch-trim-cmd-norm </input>
				<input> ap/longitudinal-cmd </input>
				<clipto>
					<min> -1.0 </min>
					<max>  1.0 </max>
				</clipto>
			</summer>

			<aerosurface_scale name="Elevator-Position-in-deg">
				<input> fcs/pitch-control-sum </input>
				<zero_centered> true </zero_centered>
				<domain>
					<min> -1.0 </min>
					<max>  1.0 </max>
				</domain>
				<range>
					<min>-20.00</min>
					<max> 15.00</max>
				</range>
				<output> fcs/elevator-pos-deg </output>
			</aerosurface_scale>
		</channel>


		<channel name="Roll">
			<summer name="fcs/roll-control-sum">
				<input> fcs/aileron-cmd-norm </input>
				<input> fcs/roll-trim-cmd-norm </input>
				<input> ap/lateral-cmd </input>
				<clipto>
					<min> -1.0 </min>
					<max>  1.0 </max>
				</clipto>
			</summer>
			<aerosurface_scale name="Left-aileron-Position-in-deg">
				<input> fcs/roll-control-sum </input>
				<zero_centered> false </zero_centered>
				<domain>
					<min> -1.0 </min>
					<max>  1.0 </max>
				</domain>
				<range>
					<min> -12.00</min>
					<max>  24.00</max>
				</range>
				<output> fcs/left-aileron-pos-deg </output>
			</aerosurface_scale>

			<aerosurface_scale name="Right-aileron-Position-in-deg">
				<input> -fcs/roll-control-sum </input>
				<zero_centered> false </zero_centered>
				<domain>
					<min> -1.0 </min>
					<max>  1.0 </max>
				</domain>
				<range>
					<min>-12.00</min>
					<max> 24.00</max>
				</range>
				<output> fcs/right-aileron-pos-deg </output>
			</aerosurface_scale>

			<summer name="fcs/total-aileron-pos-deg">
				<description> Total control deflection (Left-Right)</description>
				<input>  fcs/left-aileron-pos-deg </input>
				<input> -fcs/right-aileron-pos-deg </input>
			</summer>

		</channel>


		<channel name="Yaw">

			<!-- Datcom doesn't calculate rudder parameters, but this is included for completeness -->
			<summer name="fcs/yaw-control-sum">
				<input> fcs/rudder-cmd-norm </input>
				<input> fcs/yaw-trim-cmd-norm </input>
				<input> ap/directional-cmd </input>
				<clipto>
				<min> -1.0 </min>
				<max>  1.0 </max>
				</clipto>
			</summer>

			<aerosurface_scale name="fcs/rudder">
				<input> fcs/yaw-control-sum </input>
				<zero_centered> true </zero_centered>
				<domain>
				<min> -1.0 </min>
				<max>  1.0 </max>
				</domain>
				<range>
				<min> -20.0 </min>
				<max>  20.0 </max>
				</range>
				<output> fcs/rudder-pos-deg </output>
			</aerosurface_scale>

		</channel>


		<channel name="Flaps">
			<kinematic name="flap-control">
				<input> fcs/flap-cmd-norm </input>
				<traverse>
					<setting>
						<position>  .000 </position>			<!--    .00 deg -->
						<time>       .00 </time>
					</setting>
					<setting>
						<position>  .143 </position>			<!--   5.00 deg -->
						<time>      2.00 </time>
					</setting>
					<setting>
						<position>  .286 </position>			<!--  10.00 deg -->
						<time>      4.00 </time>
					</setting>
					<setting>
						<position>  .429 </position>			<!--  15.00 deg -->
						<time>      6.00 </time>
					</setting>
					<setting>
						<position>  .571 </position>			<!--  20.00 deg -->
						<time>      8.00 </time>
					</setting>
					<setting>
						<position>  .714 </position>			<!--  25.00 deg -->
						<time>     10.00 </time>
					</setting>
					<setting>
						<position>  .857 </position>			<!--  30.00 deg -->
						<time>     12.00 </time>
					</setting>
					<setting>
						<position> 1.000 </position>			<!--  35.00 deg -->
						<time>     14.00 </time>
					</setting>
				</traverse>
				<output> fcs/flap-pos-norm </output>
			</kinematic>

			<aerosurface_scale name="fcs/flap-position">
				<input> fcs/flap-pos-norm </input>
					<zero_centered> false </zero_centered>
					<domain>
						<min>  0.0 </min>
						<max>  1.0 </max>
					</domain>
					<range>
						<min>   .00 </min>
						<max> 35.00 </max>
					</range>
					<output> fcs/flap-pos-deg </output>
			</aerosurface_scale>

		</channel>


		<channel name="Landing Gear">
			<kinematic name="gear-control">
				<input>gear/gear-cmd-norm</input>
				<traverse>
					<setting>
						<position> 0 </position>
						<time>     0 </time>
					</setting>
					<setting>
						<position> 1 </position>
						<time>     5 </time>
					</setting>
				</traverse>
				<output>gear/gear-pos-norm</output>
			</kinematic>
		</channel>

	</flight_control>


<!-- **********************************************************************
                                    AERODYNAMICS
     *******************************************************************-->
	<!-- Link to the aerodynamics file -->
	<aerodynamics file="ProMecc-Freccia_aero.xml"/>

<!-- ********************************************************************** -->

     <!--
     We are specifying exact parameters that we want to record,
     instead of using the groups, because the contents of
     the groups may change with different versions of JSBSim,
     which screws up the parameters for the Gnuplot scripts.
     If you add parameters, add them at the end of this list.
     -->

	<output name="ProMecc-Freccia.tab" type="TABULAR" rate="20">

